<UserControl x:Class="NavBarAndPrism.Shell"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dxn="http://schemas.devexpress.com/winfx/2008/xaml/navbar"
    xmlns:dxprism="http://schemas.devexpress.com/winfx/2008/xaml/prism"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">

    <Grid x:Name="LayoutRoot" Background="White">
        <dxn:NavBarControl Name="navBarControl" dxprism:DXRegionManager.RegionName="NavBarControlRegion" VerticalAlignment="Top">
            <dxn:NavBarControl.View>
                <dxn:NavigationPaneView />
            </dxn:NavBarControl.View>
        </dxn:NavBarControl>
    </Grid>
</UserControl>
